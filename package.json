{
  "name": "boilerplate-nodejs-tdd",
  "version": "1.0.0",
  "description": "boilerplate with nodejs and tdd",
  "main": "server.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec npm run babel-node -- ./server.js",
    "start": "NODE_ENV=production nodemon --exec npm run babel-node -- ./server.js",
    "test": "NODE_ENV=test jest"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidraghetti/Boilerplate-NodeJS-TDD.git"
  },
  "keywords": [
    "nodejs",
    "tdd",
    "git"
  ],
  "author": "Guilherme Draghetti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guidraghetti/Boilerplate-NodeJS-TDD/issues"
  },
  "homepage": "https://github.com/guidraghetti/Boilerplate-NodeJS-TDD#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.5"
  }
}
